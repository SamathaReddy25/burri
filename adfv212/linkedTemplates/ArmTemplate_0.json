						"type": "date",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "SupplierReference",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "IsOrderFinalized",
						"type": "bit",
						"precision": 255,
						"scale": 255
