					{
						"name": "SupplierID",
						"type": "int",
						"precision": 10,
						"scale": 255
					},
					{
						"name": "OrderDate",
						"type": "date",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "DeliveryMethodID",
						"type": "int",
						"precision": 10,
						"scale": 255
					},
					{
						"name": "ContactPersonID",
						"type": "int",
						"precision": 10,
						"scale": 255
					},
					{
